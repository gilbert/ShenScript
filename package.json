  "devDependencies": {
    "eslint": "^5.16.0",
    "follow-redirects": "^1.7.0",
    "http-server": "^0.11.1",
    "mocha": "^5.2.0",
    "mocha-each": "^1.2.0",
    "parsimmon": "^1.12.0",
